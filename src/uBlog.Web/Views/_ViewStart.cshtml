@inject IConfigurationRoot config
@inject ISettingService settings
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Author = settings.Author;
    ViewBag.Photo = settings.Photo;
    ViewBag.Description = $"Personal blog of {settings.Author}";
    ViewBag.Keywords = "";
    ViewBag.Title = $"Blog – {settings.Author}";
    ViewBag.Version = config["version"];
}